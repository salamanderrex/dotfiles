{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "suggest": {
    "autoImports": true
  },
  "python.analysis.typeCheckingMode": "basic",
  "python.pythonPath": "/usr/local/bin/python",
  "coc.preferences.formatOnSaveFiletypes": [
    "typescript",
    "javascript",
    "typescriptreact",
    "javascriptreact",
    "json",
    "css",
    "markdown"
  ]
}
